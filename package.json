    "clsx": "^1.1.1",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "gatsby": "^3.13.1",
    "gatsby-plugin-image": "^1.14.1",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-typescript": "^3.14.0",
    "gatsby-source-contentful": "^5.14.0",
    "gatsby-theme-material-ui": "^2.0.1",
    "marked": "^3.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.42.1"
  },
  "devDependencies": {
